<h1>Listing users</h1>

<h2><%= link_to 'Export csv', export_csv_users_path %></h2>

<table>
  <tr>
    <th>Fname</th>
    <th>Lname</th>
    <th>Email</th>
    <th>Mobile</th>
    <th>Phone</th>
    <th>Dob</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>Zip</th>
    <th>State code</th>
    <th>State</th>
    <th>Country</th>
    <th>City</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.fname %></td>
    <td><%= user.lname %></td>
    <td><%= user.email %></td>
    <td><%= user.mobile %></td>
    <td><%= user.phone %></td>
    <td><%= user.dob %></td>
    <td><%= user.address1 %></td>
    <td><%= user.address2 %></td>
    <td><%= user.zip %></td>
    <td><%= user.state_code %></td>
    <td><%= user.state %></td>
    <td><%= user.country %></td>
    <td><%= user.city %></td>
  </tr>
<% end %>
</table>

<br />

